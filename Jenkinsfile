node {
   stage('Code Checkout') {
    git 'https://github.com/jsreevidya/chalo.git'
   }
   stage('Build') {
    sh 'echo Build is checked out'
   }
   stage('Test') {
    sh 'echo Test'
   }
   stage('Package') {
    sh 'echo Package'
   }
   stage('Deploy to Artifactory') {
    sh 'echo Package'
   }
   stage('Deploy to Dev') {
    sh 'echo Dev'
   }
}
